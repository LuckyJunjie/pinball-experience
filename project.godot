; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Pinball Experience"
config/description="Pinball game inspired by Flutter I/O Pinball"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")

[autoload]

GameManager="*res://scripts/GameManager.gd"
SoundManager="*res://scripts/SoundManager.gd"

[display]

window/size/viewport_width=800
window/size/viewport_height=600
window/size/mode=2
window/stretch/mode="canvas_items"

[layer_names]

2d_physics/layer_1="ball"
2d_physics/layer_2="flippers"
2d_physics/layer_3="walls"
2d_physics/layer_4="obstacles"

[physics]

2d/default_gravity_vector=Vector2(0, 980)
2d/default_linear_damp=0.0
2d/default_angular_damp=0.1
